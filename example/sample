
// a: b: c: 10 & 9 + 2 * 2 | 20 ^ - 2 | #30
	// d: a: b ^ 2 - 4 * a * c

	// b: b ^ 2 - 4 * a * c
// a {}, arg . b

// tom: bob: hello, bob . jeff

// a: {
// 	a: 10 b: 20
// 	c: 30}

quadratic: {
	a: b: c [f64]

	discriminant: b^2 - 4*a*c
	rooted: Sqrt { discriminant }
	denominator: 2*a

	positive: (-b + rooted) / denominator
	negative: (-b - rooted) / denominator
}
